{
  "name": "aie",
  "productName": "aie",
  "version": "0.0.0-github-release-setup-3",
  "description": "Electron Forge with Webpack and Typescript",
  "private": true,
  "scripts": {
    "start": "electron-webpack dev",
    "clean": "rm -r dist",
    "compile": "electron-webpack",
    "dist:all": "npm run compile && electron-builder -mwl",
    "dist:mac": "npm run compile && electron-builder -m",
    "dist:win": "npm run compile && electron-builder -w",
    "dist:linux": "npm run compile && electron-builder -l",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iluminar-yi/aie.git"
  },
  "keywords": [],
  "author": "Iluminar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iluminar-yi/aie/issues"
  },
  "homepage": "https://github.com/iluminar-yi/aie#readme",
  "build": {
    "asar": true,
    "compression": "maximum",
    "dmg": {
      "format": "ULFO"
    },
    "mac": {
      "identity": null,
      "target": [
        {
          "target": "dmg",
          "arch": "x64"
        }
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": "x64"
        }
      ]
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": "x64"
        }
      ],
      "category": "Audio"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true
    }
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "sanitize-filename": "^1.6.3",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.2",
    "source-map-support": "^0.5.10"
  },
  "devDependencies": {
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "electron": "^7.1.5",
    "electron-builder": "^21.2.0",
    "electron-webpack": "^2.7.4",
    "electron-webpack-eslint": "^5.0.1",
    "electron-webpack-ts": "^3.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "fibers": "^4.0.2",
    "node-sass": "^4.13.0",
    "sass": "^1.23.7",
    "sass-loader": "^8.0.0",
    "typescript": "^3.5.3",
    "webpack": "^4.41.2"
  }
}
